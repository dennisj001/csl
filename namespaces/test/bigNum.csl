int 
factorial ( n | rec )
{
    rec = 1;
    while ( n > 1 ) rec *= (n--) ;
    //while ( n > 1 ) rec *= n-- ;
    //d: while ( n > 1 ) rec *= n - 1 ;
    return rec ;
} 
