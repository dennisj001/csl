Dlsym libc 
void pbAssert ( argin ) 
{
    byte * pb = pbc () ; 
    if ( StringEqual ( pb, argin ) ) ps ( "\npbAssert : ok\n") ; 
    else 
    {   
        int64 la = Strlen ( argin ), lp = Strlen ( pb ) ; 
        printf ( "\nargIn != pbc" ) ; 
        ps ("\npbc :" ) ;
        dump ( pb, lp  ) ;
        ps ("\nargIn :" ) ;
        dump ( argin, la  ) ;
        printf ( "\nStrlen ( argin ) = %d :: Strlen ( pb ) = %d", la, lp ) ;
        printf ( "\nargIn  = \'%s\' \n!= pbc = \'%s\'", argin, pb ) ; 
        Error () ; 
    }  
    pbclr () ;
} 
