    a.ar [y + 1][y + 1][ 0 ] = a.ar [y ][y + 1][1] ; 
    hp ( a.ar [y ][y + 1][1] ) ; //pause () ;
    a.ar [y + 1][y + 1][1] = a.ar [y + 1][y + 1][0] ; 
    a.ar [y + 1][y + 1][ y ] = a.ar [y + 1][y + 1][1] ; 
    a.ar [y + 1][y + 1][y + 1] = a.ar [y + 1][y + 1][y] ; 
    hp ( &a.ar ) ;
    cadump ( &a.ar 27 ) ; 
