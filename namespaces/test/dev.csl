int
fibPTC4? ( REG n | REG fn, REG fn1, REG fn2 )
{
    fn = 0, fn1 = 0, fn2 = 1 ; 
    do 
    {   
        fn1 = fn2 ;
        fn2 = fn ;
        fn = fn1 + fn2 ; 
        n -- ;
    }
    while ( n > 0 ) ;
    return fn ;
}
